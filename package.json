{
  "name": "RNBase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appIdSuffix=development",
    "ios": "react-native run-ios --scheme '[DEV] RNBase'",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:stag": "npx react-native run-android --variant=stagingrelease --appIdSuffix=staging",
    "android:prod": "npx react-native run-android --variant=release",
    "android-build:stag": "cd android && fastlane build type:staging && cd ..",
    "android-build:prod": "cd android && fastlane build type:release && cd ..",
    "android-build:all": "yarn android-build:stag && yarn android-build:prod",
    "android-bundle": "cd android && fastlane build type:release bundle:true && cd ..",
    "start": "react-native start",
    "start:pure": "npx react-native start --reset-cache",
    "postinstall": "run-script-os",
    "postinstall:macos": "patch-package && cd ios && pod install && cd ..",
    "postinstall:default": "patch-package",
    "premake": "yarn add @bam.tech/react-native-make --dev",
    "make:splash": "yarn premake && npx react-native set-splash --path src/assets/images/App/splash.png --resize cover --background \"#222222\" && yarn aftermake",
    "make:icon": "yarn premake && npx react-native set-icon --platform ios --path src/assets/images/App/iconIos.png && npx react-native set-icon --platform android --path src/assets/images/App/iconAndroid.png && rm -rf android/app/src/main/res/mipmap-hdpi-v26 android/app/src/main/res/mipmap-ldpi-v26 android/app/src/main/res/mipmap-mdpi-v26 android/app/src/main/res/mipmap-xhdpi-v26 android/app/src/main/res/mipmap-xxhdpi-v26 android/app/src/main/res/mipmap-xxxhdpi-v26 && yarn aftermake",
    "aftermake": "yarn remove @bam.tech/react-native-make",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx --fix --max-warnings 0"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.0.1",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^7.6.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-firebase/analytics": "^19.1.0",
    "@react-native-firebase/app": "^19.1.0",
    "@react-native-firebase/messaging": "^19.1.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.2",
    "@sentry/react-native": "^5.20.0",
    "axios": "^1.6.8",
    "i18next": "^23.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nativewind": "2.0.10",
    "qs": "^6.12.0",
    "react": "18.2.0",
    "react-hook-form": "^7.51.1",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.6",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.13.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-localize": "^3.0.6",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-navigation-bar-color": "^2.0.2",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@react-navigation/devtools": "^6.0.26",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.4",
    "@types/color": "^3.0.6",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/qs": "^6.9.14",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "4.3.8",
    "jest": "^29.6.3",
    "metro-react-native-babel-transformer": "^0.77.0",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-native-flipper": "0.164.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.3",
    "rn-async-storage-flipper": "^0.0.10",
    "run-script-os": "^1.1.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "./jest-setup.tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|@react-native-community|@react-navigation)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/components/Common/Icon/getIconType.ts",
      "/src/containers/screens/Main/Home/Example/",
      "/src/components/Common/BottomSheet/components/",
      "/src/core/configs",
      "/src/core/plugins",
      "/src/core/utils/Redux.ts"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.ts"
    }
  }
}